{
  "name": "website-benwhite",
  "version": "1.0.0",
  "description": "Website for benwhite.com.au",
  "author": "Ben White",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "11ty:build": "npx @11ty/eleventy",
    "11ty:watch": "npx @11ty/eleventy --watch --quiet --ignore-initial",
    "11ty:benchmark": "DEBUG=Eleventy:Benchmark* npm run build",
    "util:rimraf": "npx rimraf _site",
    "util:killport": "npx kill-port --port 8787",
    "wrangler:dev": "npx wrangler dev --live-reload",
    "build": "npm-run-all util:rimraf 11ty:build",
    "start": "npm-run-all util:rimraf util:killport 11ty:build --parallel 11ty:watch wrangler:dev",
    "deploy": "npx wrangler deploy"
  },
  "dependencies": {
    "@11ty/eleventy": "3.1.2",
    "@11ty/eleventy-plugin-bundle": "^3.0.6",
    "@11ty/eleventy-plugin-rss": "github:11ty/eleventy-plugin-rss",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@formkit/tempo": "^0.1.2",
    "@justinribeiro/lite-youtube": "^1.8.2",
    "autoprefixer": "^10.4.21",
    "cheerio": "^1.1.2",
    "cssnano": "^7.1.0",
    "esbuild": "^0.25.8",
    "html-minifier-terser": "^7.2.0",
    "markdown-it": "^14.1.0",
    "postcss": "^8.5.6",
    "postcss-discard-comments": "^7.0.4",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.4",
    "xml-formatter": "^3.6.6"
  },
  "devDependencies": {
    "kill-port": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "wrangler": "^4.27.0"
  }
}
